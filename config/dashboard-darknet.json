{
	"version": 1,
	"allow_edit": true,
	"plugins": [],
	"panes": [
		{
			"title": "Camera Snapshot",
			"width": 1,
			"row": {
				"2": 1,
				"3": 1,
				"4": 1
			},
			"col": {
				"2": 2,
				"3": 1,
				"4": 3
			},
			"col_width": "1",
			"widgets": [
				{
					"type": "html",
					"settings": {
						"html": "imgstr = datasources[\"Detection Result\"][\"msg\"][\"bytes\"]\ns = \"<img src=\\\"data:image/jpeg;base64,\" + imgstr + \"\\\" width=100% height=100%>\"\nreturn s\n\n",
						"height": 4
					}
				}
			]
		},
		{
			"title": "Inference Result",
			"width": 1,
			"row": {
				"2": 1,
				"3": 1
			},
			"col": {
				"2": 1,
				"3": 2
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "html",
					"settings": {
						"html": "s = datasources[\"Detection Result\"][\"msg\"][\"annotations\"].map(function(r) {                      \n    return (r[\"label\"] + \": \" + r[\"confidence\"] + \"<br />\")\n}).join().replace(/,/g, \"\")\nconsole.log(s)\nreturn s",
						"height": 4
					}
				}
			]
		}
	],
	"datasources": [
		{
			"name": "Detection Result",
			"type": "paho_mqtt",
			"settings": {
				"server": "localhost",
				"port": 3000,
				"use_ssl": false,
				"client_id": "freeboard_darknetres",
				"topic": "berrynet/engine/darknet/result",
				"json_data": true,
				"username": "",
				"password": "",
				"name": "Detection Result"
			}
		}
	],
	"columns": 3
}
